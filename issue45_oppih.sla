            <nbhyphen/>
            <nbhyphen/>
            <ITEXT FONT="AR PL UMing CN Light" FONTSIZE="12" FEATURES="inherit" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="5" TXTSHY="-5" TXTOUT="1" TXTULP="-0.1" TXTULW="-0.1" TXTSTP="-0.1" TXTSTW="-0.1" SCALEH="100" SCALEV="100" BASEO="0" KERN="0" CH="比如说，社交网络工具和监控工具。4.5的提升之一就是Plasmoids的功能和稳定性都比以前强很多。"/>
            <para PARENT="Basic Text + indent"/>
